<?xml version="1.0" encoding="utf-8"?>
<PreferenceScreen
    xmlns:android="http://schemas.android.com/apk/res/android">
    <PreferenceCategory
        android:title="@string/whisperpush_title"
        android:key="whisperpush">
        <Preference
            android:title="@string/whisperpush_registration_title"
            android:key="whisperpush_registration"
            android:summary="@string/whisperpush_registration_summary">
            <intent
                android:targetPackage="org.whispersystems.whisperpush"
                android:targetClass="org.whispersystems.whisperpush.ui.RegistrationActivity" />
        </Preference>
        <Preference
            android:title="@string/whisperpush_identitykey_title"
            android:key="whisperpush_review_identities">
            <intent
                android:targetPackage="org.whispersystems.whisperpush"
                android:targetClass="org.whispersystems.whisperpush.ui.ReviewIdentitiesActivity" />
        </Preference>
    </PreferenceCategory>
</PreferenceScreen>